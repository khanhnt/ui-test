<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="EXOGTN Functional Test" verbose="1">
		<test name="exogtn-func" enabled="true">
		<packages>
			<package name="org.exoplatform.selenium.platform.exogtn.functional.*" >
				<exclude name ="org.exoplatform.selenium.platform.exogtn.functional.portalnavigation.edit"/>
