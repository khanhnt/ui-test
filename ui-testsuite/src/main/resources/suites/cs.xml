<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="CS Functional Test" verbose="1">
	<test name="cs-func" enabled="true">
		<groups>
		  <run>
		    <exclude name="pending"/>
		  </run>
		</groups>
		<packages>
			<package name="org.exoplatform.selenium.platform.cs.functional.*" />
		</packages>
	</test>

	<listeners>
    <listener class-name="org.exoplatform.selenium.TestReportListener"/>
  </listeners>
</suite>
