<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Platform Functional Test" verbose="1" preserve-order="true">
	<test name="forum-answer" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
				<!-- <exclude name="error" /> -->
			</run>
		</groups>
		<packages>
			<package name="org.exoplatform.selenium.platform.forum.functional.answer.*" />
		</packages> 
	</test>
	<listeners>
		<listener class-name="org.exoplatform.selenium.TestReportListener" />
	</listeners>
</suite>